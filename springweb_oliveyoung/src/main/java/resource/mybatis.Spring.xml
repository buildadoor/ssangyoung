<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<!-- 1. 사용할(XXXMapper.xml) 패키지명 vo클래스 alias 선언 -->
	<typeAliases>
		<typeAlias alias="ovMember" type="springweb.vo.OvMember"/>
		<typeAlias alias="ovProduct" type="springweb.vo.OvProduct"/>
	</typeAliases>
<!-- 2. 사용할 XXXMapper.xml을 추가(dao와 mapper1:1) -->
	<mappers>
		<mapper resource="resource/A01_MemberMapper.xml"/>
		<mapper resource="resource/A02_ProductMapper.xml"/>
	</mappers>
</configuration>