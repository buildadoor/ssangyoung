package springweb.z02_oliveyoung.a01_controller;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.ModelAttribute;

import springweb.z02_oliveyoung.a02_service.A01_MemberService;


@Controller
public class A01_MemberController {
	@Autowired
	private A01_MemberService service;
	@ModelAttribute("deptCom")
	public List<OvMember> getOvMemberComb(){
		return service.getDeptComb();
	}
}
